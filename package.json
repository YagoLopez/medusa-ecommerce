{
  "name": "medusa-ecommerce",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "install:backend": "yarn --cwd backend install",
    "install:admin": "yarn --cwd admin install",
    "install:storefront": "yarn --cwd storefront install",
    "start:backend": "yarn --cwd backend start",
    "start:admin": "yarn --cwd admin start",
    "start:frontend": "yarn --cwd storefront start",
    "dev:frontend": "yarn --cwd storefront dev",
    "start": "run-p start:backend start:admin start:frontend",
    "dev": "run-p start:backend start:admin dev:frontend"
  }
}
